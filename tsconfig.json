{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@auth/*": ["./auth/*"],
      "@config/*": ["./config/*"],
      "@constants/*": ["@shared/constants/*"],
      "@contexts/*": ["./contexts/*"],
      "@domain/*": ["./@domain/*"],
      "@features/*": ["./features/*"],
      "@infrastructure/*": ["./@infrastructure/*"],
      "@navigation/*": ["./navigation/*"],
      "@services/*": ["./@services/*"],
      "@sync/*": ["./@sync/*"],
      "@shared/*": ["./@shared/*"],
      "_mockData/*": ["./_mockData/*"],
      "_types/*": ["./types/*"]
    }
  }
}
